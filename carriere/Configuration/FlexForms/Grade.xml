<?xml version="1.0" encoding="UTF-8"?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Grade</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>


                    <settings.titreItem>
                        <TCEforms>
                            <label>Nom du Grade</label>
                            <config>
                                <type>input</type>
                                <size>50</size>
                            </config>
                        </TCEforms>
                    </settings.titreItem>

                    <settings.categoriePromo>
                        <TCEforms>
                            <label>Decret</label>
                            <config>
                                <type>check</type>
                                <renderType>selectSingle</renderType>
                                <items>
                                    <numIndex index="0">
                                        <numIndex index="0">Non</numIndex>
                                        <numIndex index="1">Non</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">Gauche</numIndex>
                                        <numIndex index="1">Gauche</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">Centre</numIndex>
                                        <numIndex index="1">Centre</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">Droite</numIndex>
                                        <numIndex index="1">Droite</numIndex>
                                    </numIndex>
                                </items>
                                <default>Non</default>
                            </config>
                        </TCEforms>
                    </settings.categoriePromo>


                    <section>
                        <type>array</type>
                        <section>1</section>
                        <title>Contenu</title>
                        <el>

                            <item>
                                <type>array</type>
                                <title>Cadre simple</title>
                                <el>
                                    <titre>
                                        <TCEforms>
                                            <label>Titre</label>
                                            <config>
                                                <type>input</type>
                                                <size>30</size>
                                            </config>
                                        </TCEforms>
                                    </titre>

                                    <lienFonction>
                                        <TCEforms>
                                            <label>Lien vers la page fonction</label>
                                            <config>
                                                <type>input</type>
                                                <renderType>inputLink</renderType>
                                            </config>
                                        </TCEforms>
                                    </lienFonction>

                                    <lienEchelle>
                                        <TCEforms>
                                            <label>Lien vers la page Ã©chelle indiciaire (Voir comment faire)</label>
                                            <config>
                                                <type>input</type>
                                                <renderType>inputLink</renderType>
                                            </config>
                                        </TCEforms>
                                    </lienEchelle>
                                </el>
                            </item>
                        </el>
                    </section>


                </el>

            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>